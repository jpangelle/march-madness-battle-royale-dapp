{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "startOld": "react-scripts start",
    "buildOld": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.ts{,x}'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts{,x}'",
    "build": "tsc --build",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess 'node --es-module-specifier-resolution=node ./dist/index'",
    "start": "node --es-module-specifier-resolution=node ./dist/index",
    "prepare": "cd .. && husky install client/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "6.3.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0"
  },
  "main": "index.js",
  "license": "MIT"
}
